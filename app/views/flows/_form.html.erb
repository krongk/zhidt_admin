<%= simple_form_for(@flow) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :package %>
    <%= f.input :title %>
    <%= f.input :description %>
    <%= f.input :tutorial %>
    <%= f.input :icon %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
