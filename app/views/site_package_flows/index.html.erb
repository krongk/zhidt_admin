<h1>Listing site_package_flows</h1>

<table>
  <thead>
    <tr>
      <th>Site package</th>
      <th>Flow</th>
      <th>Content</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @site_package_flows.each do |site_package_flow| %>
      <tr>
        <td><%= site_package_flow.site_package %></td>
        <td><%= site_package_flow.flow %></td>
        <td><%= site_package_flow.content %></td>
        <td><%= site_package_flow.status %></td>
        <td><%= link_to 'Show', site_package_flow %></td>
        <td><%= link_to 'Edit', edit_site_package_flow_path(site_package_flow) %></td>
        <td><%= link_to 'Destroy', site_package_flow, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Site package flow', new_site_package_flow_path %>
